package main

import (
	"fmt"
	"gopkg.in/h2non/gock.v1"
	"io/ioutil"
	"net/http"
)

func main() {
	defer gock.Disable()
	
	# Mock 
	gock.New("http://httpbin.org").
		Get("/foo").
		Reply(201).
		SetHeader("Server", "gock")

	res, err := http.Get("http://httpbin.org/bar")
	if err != nil {
		fmt.Printf("Error: %s\n", err)
	}
}
